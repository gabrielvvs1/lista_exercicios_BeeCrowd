#include <stdio.h>
int main(){

    double raio , area , pi;

    pi= 3.14159;

    scanf("%lf", &raio); 

    area = pi*raio*raio;

    printf("A=%.4lf\n", area);

    /* add .4 antes do "lf"na saida caso queira 4 algarismos depois do "."
        assim sucessivamente quando quiser qualquer numero depois do "."
    */

return 0;
}
